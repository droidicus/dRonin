<xml>
	<object name="SystemIdent" singleinstance="true" settings="true">
		<description>The input to the PID tuning.</description>
		<field name="Tau" units="s" type="float" elements="1" defaultvalue="0"/>
		<field name="Beta" units="" type="float" elementnames="Roll,Pitch,Yaw" defaultvalue="0"/>
		<field name="Bias" units="" type="float" elementnames="Roll,Pitch,Yaw" defaultvalue="0"/>
		<field name="Noise" units="(deg/s)^2" type="float" elementnames="Roll,Pitch,Yaw" defaultvalue="0"/>
		<field name="Period" units="ms" type="float" elements="1" defaultvalue="0"/>
		<field name="NumAfPredicts" units="" type="uint32" elements="1" defaultvalue="0"/>
		<field name="NumSpilledPts" units="" type="uint32" elements="1" defaultvalue="0"/>
		<field name="HoverThrust" units="% / 100" type="float" elements="1" defaultvalue="0"/>
		<field name="AccumIndex" units="" type="uint32" elements="1" defaultvalue="0"/>
		<field name="GyroAccumRoll" units="deg/s" type="float" elements="6" defaultvalue="0"/>
		<field name="GyroAccumPitch" units="deg/s" type="float" elements="6" defaultvalue="0"/>
		<field name="GyroAccumYaw" units="deg/s" type="float" elements="6" defaultvalue="0"/>
		<field name="ActuatorDesiredAccumRoll" units="% / 100" type="float" elements="6" defaultvalue="0"/>
		<field name="ActuatorDesiredAccumPitch" units="% / 100" type="float" elements="6" defaultvalue="0"/>
		<field name="ActuatorDesiredAccumYaw" units="% / 100" type="float" elements="6" defaultvalue="0"/>
		<field name="ActuatorDesiredAccumThrust" units="% / 100" type="float" elements="6" defaultvalue="0"/>
		<access gcs="readwrite" flight="readwrite"/>
		<telemetrygcs acked="false" updatemode="manual" period="0"/>
		<telemetryflight acked="false" updatemode="onchange" period="1000"/>
		<logging updatemode="manual" period="0"/>
	</object>
</xml>
